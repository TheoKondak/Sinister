theme: herman
herman:
	extraDocs:
		- './my-file.md'
	groups:
	  # all items will be listed in the order given...
	  dev: Development Only
	  utility: Generic Utility
	sass:
		includePaths:
		- 'static/sass'
		use:
		  - 'config/tools'
		  - file: 'config/other-tools'
			namespace: 'my-tools'